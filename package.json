{
  "name": "api",
  "private": true,
  "version": "0.0.1",
  "author": "instintos",
  "license": "UNLICENSED",
  "scripts": {
    "graphql:generate": "ts-node ./src/shared/graphql/generateTypes.ts",
    "prisma:generate": "prisma generate --schema=./src/shared/prisma/schema.prisma --watch",
    "prisma:update-uniques": "prisma db push --schema=\"./src/shared/prisma/schema.prisma\"",
    "dev": "concurrently \"yarn:start:dev\" \"yarn:graphql:generate\" \"yarn:prisma:generate\" \"yarn:prisma:update-uniques\"",
    "build": "nest build",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "prod": "node dist/main",
    "start:dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "clear && jest --verbose --config jest.config.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "clear && jest --config jest.e2e.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.19",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/platform-express": "^9.0.0",
    "apollo-server-core": "^3.10.0",
    "apollo-server-express": "^3.10.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "class-validator-mongo-object-id": "^1.3.0",
    "graphql": "^16.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-morph": "^15.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@prisma/client": "^4.2.1",
    "@types/babel__core": "^7.1.19",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "mongodb": "^4.8.1",
    "prettier": "^2.3.2",
    "prisma": "^4.2.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  }
}
